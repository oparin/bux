                                        {{ form_label(form.name, null, {'label_attr': {'class': 'control-label col-lg-4'}}) }}
                                        <div class="col-lg-4">
                                            {{ form_widget(form.name, {'attr': {'class': 'form-control'}}) }}
                                            {% if form_errors(form.name) %}
                                                <span id="required2-error" class="help-block">{{ form_errors(form.name) }}</span>
                                            {% endif %}
                                        </div>
                                    </div><!-- /.form-group -->

                                    <div class="form-group {% if form_errors(form.duration) %}has-error{% endif %}">
                                        {{ form_label(form.duration, null, {'label_attr': {'class': 'control-label col-lg-4'}}) }}

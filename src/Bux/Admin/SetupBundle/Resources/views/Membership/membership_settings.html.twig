                                            {{ form_widget(form.duration, {'attr': {'class': 'form-control', 'placeholder': '0'}}) }}
                                            <div style="position: absolute; margin-top: -27px; margin-left: 300px">days</div>
                                            {% if form_errors(form.duration) %}
                                                <span id="required2-error" class="help-block">
                                                    {{ form_errors(form.duration) }}

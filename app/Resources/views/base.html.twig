                        <ul>
                            {% for visitor in visit %}
                                {#{{ dump(visitor.country) }}#}
                                <li>
                                    <div style="float: left">
                                        <img width="32" alt="thumb" src="{{ asset('images/flag/' ~ visitor.flag) }}" class="thinframess">
                                    </div>
                                    <div style="margin-top: 8px; margin-left: 50px">New member {{ visitor.name }} from <br>{{ visitor.country }}</div>
                                </li>
                            {% endfor %}
                        </ul>
                    </div>
                </div>

                {% set visit = visitors.registr %}

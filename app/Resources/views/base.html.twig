                    <div class="content-box bt-space10">
                        {#<div class="box-body">#}
                        <div class="box-wrap clear  box-slide-body">
                            <div style="text-align: left">
                                {% for ban in banner %}
                                    <div style="padding: 2px; border: 1px solid #808080; float: left; width: 470px; height: 62">
